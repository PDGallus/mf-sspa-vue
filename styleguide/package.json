{
  "scripts": {
    "dev": "vite build && npm run dev:concurrently",
    "dev:concurrently": "concurrently --kill-others \"npm run build:local\" \"npm run serve\"",
    "build": "npm run type-check && vite build",
    "build:local": "vite build --watch --mode dev",
    "type-check": "vue-tsc --noEmit",
    "serve": "vite preview",
    "eslint": "eslint src/** --fix"
  },
  "devDependencies": {
    "@types/node": "22.15.35",
    "@types/systemjs": "6.15.3",
    "concurrently": "9.2.0",
    "globals": "16.3.0",
    "husky": "9.1.7",
    "postcss-nesting": "13.0.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0",
    "vite": "7.1.2",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.0.5"
  },
  "dependencies": {
    "single-spa-vue": "3.0.1",
    "vue": "3.5.18",
    "vue-router": "4.5.1"
  }
}
